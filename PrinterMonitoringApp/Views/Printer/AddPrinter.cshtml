
@{
    ViewBag.Title = "Add Printer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/bundles/addprinters")
<aside class="content-wrapper sidebarLeft">
    <div class="content container-fluid sidebarRight animated fadeInUp">
        <div class="row">

            <div class="col-sm-12">
                <div class="panel panel-white">
                    <div class="panel-body content-title-container">
                        <div class="right">
                            <ol class="breadcrumb breadcrumb-theme breadcrumb-sm breadcrumb-red">
                                <li><a href="~/Home/Dashboard">Dashboard</a></li>
                                <li><a>Printer Management</a></li>
                                <li class="active"><a>Add Printers</a></li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="row">
            <div class="col-sm-12">
                <div class="panel panel-white">
                    <div class="panel-heading">
                        <h3 class="panel-title">Add Printer</h3>
                    </div>
                    <div class="panel-body">
                        <div id="form-validation" class="form-horizontal" role="form">
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Printer UID</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" placeholder="Enter Printer Unique Identifier" name="printerUID" id="printerUID" autofocus="" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Printer Serial No</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" placeholder="Enter Printer Serial Number" name="printerSrNo" id="printerSrNo" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Printer Name</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" placeholder="Enter Printer Name" name="printerName" id="printerName" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Printer Brand</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" placeholder="Enter Printer Brand" name="printerBrand" id="printerBrand" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Branch</label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="printerBranch"></select>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-offset-2 col-sm-10">
                                    <button type="submit" id="addBtn" class="btn btn-red" style="float:right;" onclick="addPrinter()"><i class="fa fa-cog"></i> Add</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</aside>






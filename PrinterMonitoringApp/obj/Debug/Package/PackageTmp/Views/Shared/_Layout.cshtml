                <!-- End Offline Section -->

            </ul>
            <!-- End Chatlist Wrapper -->

        </div>
        <!-- End Sidebar Wrapper -->

    </aside>
    <!-- End Right Sidebar -->
    @RenderBody()
    
    @RenderSection("scripts", required: false)
</body>
